username BKNJ-NetAdmin privilege 15 
enable secret NetAdmin123@BKNJ

ip ftp username BKNJ-NetAdmin
ip ftp password 7 NetAdmin123@BKNJ

---------------------------------------------------


enable 
configure Terminal

hostname bknj-co-ssw1

ine console 0
logging synchronous
login local
exit

line vty 0 4
logging synchronous
login local
exit

logging console 
logging buffered 8192
logging host 172.16.100.4
logging host 172.16.100.5
logging on
logging trap debbuging
logging userinfo

spanning-tree portfast bpduguard default
spanning-tree mode rapid-pvst
spanning-tree vlan 1-4094 priority 28672

lldp run
no cdp run

interface vlan 1
no ip address
shut down
exit

interface vlan 3
ip address 172.16.3.8 255.255.255.240
no shutdown
exit
 
vlan 3
name Net_Management
exit

vlan 10
name BKNJ_Guests
exit

vlan 20
name BKNJ_Devices
exit

vlan 30
name BKNJ_Security
exit

vlan 60
name BKNJ_employees
exit

vlan 100
name BKNJ_infra
exit

interface gigabitethernet0/1
description ## to BKNJ-CO-DGW01:gi1/1/3 ##
switchport mode trunk
switchport trunk allowed vlan all
no shutdown
exit

interface gigabitethernet1/1
description ## to BKNJ-CO-SSW02:gi1/1 ##
switchport mode trunk
switchport trunk allowed vlan all
no shutdown
exit

interface gigabitethernet8/1
description ## to BKNJ-CO-SRV1 ##
switchport mode access
switchport acces vlan 100
lldp transmit
lldp receive
no shutdown
exit


ntp authenticate
ntp authentication-key 1 md5 NetAdmin123@BKNJ
ntp trusted-key 1
ntp server 172.16.100.5 key 1
ntp update-calendar


end


write memory
