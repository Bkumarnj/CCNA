username BKNJ-NetAdmin privilege 15 
enable secret NetAdmin123@BKNJ

ip ftp username BKNJ-NetAdmin
ip ftp password 7 NetAdmin123@BKNJ

---------------------------------------------------


enable 
configure terminal

hostname bknj-co-ia-asw1

ine console 0
logging synchronous
login local
exit

line vty 0 4
logging synchronous
login local
exit

logging console 
logging buffered 8192
logging host 172.16.100.4
logging host 172.16.100.5
logging on
logging trap debbuging
logging userinfo

spanning-tree portfast bpduguard default
spanning-tree mode rapid-pvst

lldp run

interface vlan 1
no ip address
shut down
exit

interface vlan 3
ip address 172.16.3.10 255.255.255.240
no shutdown
exit
 
vlan 3
name Net_Management
exit

vlan 10
name BKNJ_Guests
exit

vlan 20
name BKNJ_Devices
exit

vlan 30
name BKNJ_Security
exit

vlan 60
name BKNJ_employees
exit

vlan 100
name BKNJ_infra
exit

interface range gigabitethernet0/1-2
switchport mode trunk
switchport trunk allowed vlan all
lldp receive
lldp transmit
exit

interface gigabitethernet0/1
description ## to BKNJ-CO-DGW1:gi1/0/21 ##
exit

interface gigabitethernet0/2
description ## to BKNJ-CO-DGW2:gi1/0/22 ##
exit

interface range fastethernet0/1-6
switchport mode access
switchport access vlan 10
spanning-tree portfast
lldp receive
lldp transmit
exit

interface range fastethernet0/7-12
switchport mode access
switchport access vlan 20
spanning-tree portfast
lldp receive
lldp transmit
exit

interface range fastethernet0/13-18
switchport mode access
switchport access vlan 30
spanning-tree portfast
lldp receive
lldp transmit
exit

interface range fastethernet0/19-24
switchport mode access
switchport access vlan 60
spanning-tree portfast
lldp receive
lldp transmit
exit

ntp authenticate
ntp authentication-key 1 md5 NetAdmin123@BKNJ
ntp trusted-key 1
ntp server 172.16.100.5 key 1
ntp update-calendar


end

write memory

